@article{Sirunyan_2018,
	doi = {10.1088/1748-0221/13/10/p10005},
	url = {https://doi.org/10.1088/1748-0221/13/10/p10005},
	year = 2018,
	month = {oct},
	publisher = {{IOP} Publishing},
	volume = {13},
	number = {10},
	pages = {P10005--P10005},
    author         = "A.M. Sirunyan and A. Tumasyan and others",
	title = {Performance of reconstruction and identification of $\tau$ leptons decaying to hadrons and $\nu_\tau$ in pp collisions at $\sqrt{s}$ =13 {TeV}},
	journal = {Journal of Instrumentation},
	abstract = {The algorithm developed by the CMS Collaboration to reconstruct and identify œÑ leptons produced in proton-proton collisions at ‚àös=7 and 8 TeV, via their decays to hadrons and a neutrino, has been significantly improved. The changes include a revised reconstruction of œÄ0 candidates, and improvements in multivariate discriminants to separate œÑ leptons from jets and electrons. The algorithm is extended to reconstruct œÑ leptons in highly Lorentz-boosted pair production, and in the high-level trigger. The performance of the algorithm is studied using proton-proton collisions recorded during 2016 at ‚àös=13 TeV, corresponding to an integrated luminosity of 35.9 fb-1. The performance is evaluated in terms of the efficiency for a genuine œÑ lepton to pass the identification criteria and of the probabilities for jets, electrons, and muons to be misidentified as œÑ leptons. The results are found to be very close to those expected from Monte Carlo simulation.}
}
